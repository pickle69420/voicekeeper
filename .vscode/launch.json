{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Next.js",
      "type": "pwa-node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "port": 9229,
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Attach to Node",
      "type": "pwa-node",
      "request": "attach",
      "port": 9229
    }
  ]
}
